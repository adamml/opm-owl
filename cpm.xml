<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY op "http://environment.data.gov.au/def/op#" >
    <!ENTITY vann "http://purl.org/vocab/vann/" >
]>


<rdf:RDF xmlns="http://onto.nerc.ac.uk/cpm#"
     xml:base="http://onto.nerc.ac.uk/cpm"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:op="http://environment.data.gov.au/def/op#"
     xmlns:vann="http://purl.org/vocab/vann/">
    <owl:Ontology rdf:about="http://onto.nerc.ac.uk/cpm">
        <rdfs:label xml:lang="en">
            Complex Property Model ontology
        </rdfs:label>
        <rdfs:comment xml:lang="en">
            An OWL representation of the Complex Property Model (CPM).

            The ontology here encodes the CPM defined in the INSPIRE
            guidelines for Observations &amp; Measurements and Sensor Web
            Enablement-related standards document.
        </rdfs:comment>
        <vann:preferredNamespacePrefix>cpm</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>http://onto.nerc.ac.uk/cpm#</vann:preferredNamespaceUri>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->

    <owl:AnnotationProperty rdf:about="&skos;definition"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#notation -->

    <owl:AnnotationProperty rdf:about="&skos;notation"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://vocab.nerc.ac.uk/datatypes/datatypes.xml#OPMClassName -->

    <rdfs:Datatype rdf:about="http://vocab.nerc.ac.uk/datatypes/datatypes.xml#OPMClassName"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://onto.nerc.ac.uk/cpm#StatisticalMeasure -->

    <rdf:Description rdf:about="http://onto.nerc.ac.uk/cpm#StatisticalMeasure">
        <rdfs:label xml:lang="en">Statistical Measure</rdfs:label>
        <skos:notation rdf:datatype="http://vocab.nerc.ac.uk/datatypes/datatypes.xml#OPMClassName">
            Statistical Measure
        </skos:notation>
        <skos:definition xml:lang="en">
            A description of some statistical measure e.g. &quot;daily maximum&quot;.
            The measure is usually some function over some time (e.g. an hour,
            a day) or space (e.g. a length, area or volume). Other aggregation
            types can be supported by the &apos;otherAggregation&apos; extension point.
        </skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
    </rdf:Description>
    


    <!-- http://onto.nerc.ac.uk/cpm#UnitOfMeasure -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#UnitOfMeasure"/>
    


    <!-- http://onto.nerc.ac.uk/cpm#categoryComparison -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#categoryComparison">
        <rdfs:label xml:lang="en">comparison</rdfs:label>
        <skos:definition xml:lang="en">
            A comparison operator. In the case of a category constraint it should be 
            &quot;equalTo&quot; or &quot;notEqualTo&quot;
        </skos:definition>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#CategoryConstraint"/>
        <rdfs:range rdf:resource="http://onto.nerc.ac.uk/cpm#ComparisonOperatorValue"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#comparison"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#comparison -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#comparison">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#component -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#component">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:range rdf:resource="http://onto.nerc.ac.uk/cpm#AbstractObservableProperty"/>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#CompositeObservableProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#derivedFrom -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#derivedFrom">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:label xml:lang="en">derivedFrom</rdfs:label>
        <skos:definition xml:lang="en">
            One statistical measure may be derived from another. e.g. Monthly Maximum 
            Temperature may be derived from Daily Mean Temperatures.
        </skos:definition>
        <rdfs:range rdf:resource="http://onto.nerc.ac.uk/cpm#StatisticalMeasure"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#endComparison -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#endComparison">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:label xml:lang="en">endComparison</rdfs:label>
        <skos:definition xml:lang="en">
            The comparator used for the upper range limit (e.g. lessThan)
        </skos:definition>
        <rdfs:range rdf:resource="http://onto.nerc.ac.uk/cpm#ComparisonOperatorValue"/>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#RangeBounds"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#opUnitOfMeasure -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#opUnitOfMeasure">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:label xml:lang="en">uom</rdfs:label>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#ObservableProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#UnitOfMeasure"/>
        <rdfs:range rdf:resource="http://data.qudt.org/qudt/owl/1.0.0/qudt.owl#Unit"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#rangeUnitOfMeasure -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#rangeUnitOfMeasure">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:label xml:lang="en">uom</rdfs:label>
        <skos:definition xml:lang="en">Units of measure used in the constraint</skos:definition>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#RangeConstraint"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#UnitOfMeasure"/>
        <rdfs:range rdf:resource="http://data.qudt.org/qudt/owl/1.0.0/qudt.owl#Unit"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#rangeValue -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#rangeValue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:label xml:lang="en">value</rdfs:label>
        <skos:definition xml:lang="en">
            The numerical value range of the proprty that is constrained
        </skos:definition>
        <rdfs:range rdf:resource="http://onto.nerc.ac.uk/cpm#RangeBounds"/>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#RangeConstraint"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#restriction -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#restriction">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:label xml:lang="en">restriction</rdfs:label>
        <rdfs:range rdf:resource="http://onto.nerc.ac.uk/cpm#Constraint"/>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#ObservableProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#scalarComparison -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#scalarComparison">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:label xml:lang="en">comparison</rdfs:label>
        <skos:definition xml:lang="en">
            The comparison to be used in the constraint, e.g. greaterThan
        </skos:definition>
        <rdfs:range rdf:resource="http://onto.nerc.ac.uk/cpm#ComparisonOperatorValue"/>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#ScalarConstraint"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#comparison"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#scalarUnitOfMeasure -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#scalarUnitOfMeasure">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:label xml:lang="en">uom</rdfs:label>
        <skos:definition xml:lang="en">
            Units of measure used in the constraint
        </skos:definition>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#ScalarConstraint"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#UnitOfMeasure"/>
        <rdfs:range rdf:resource="http://data.qudt.org/qudt/owl/1.0.0/qudt.owl#Unit"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#startComparison -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#startComparison">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:label xml:lang="en">startComparison</rdfs:label>
        <skos:definition xml:lang="en">
            The comparator used for the lower range limit (e.g. greaterThanOrEqualTo)
        </skos:definition>
        <rdfs:range rdf:resource="http://onto.nerc.ac.uk/cpm#ComparisonOperatorValue"/>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#RangeBounds"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#statisticalMeasure -->

    <owl:ObjectProperty rdf:about="http://onto.nerc.ac.uk/cpm#statisticalMeasure">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:label xml:lang="en">statisticalMeasure</rdfs:label>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#ObservableProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#StatisticalMeasure"/>
    </owl:ObjectProperty>
    


    <!-- http://def.seegrid.csiro.au/static/isotc211/iso19156/2011/observation.rdf#observedProperty -->

    <owl:ObjectProperty rdf:about="http://def.seegrid.csiro.au/static/isotc211/iso19156/2011/observation.rdf#observedProperty">
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#AbstractObservableProperty"/>
        <rdfs:range rdf:resource="http://def.seegrid.csiro.au/static/isotc211/iso19156/2011/observation.rdf#Observation"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://onto.nerc.ac.uk/cpm#aggregation -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#aggregation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#StatisticalMeasure"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#aggregationArea -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#aggregationArea">
        <rdfs:label xml:lang="en">aggregationArea</rdfs:label>
        <skos:definition xml:lang="en">
            A two dimensional spatial range over which a statistic is calculated, for example
            one square metre.
        </skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#aggregation"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#aggregationLength -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#aggregationLength">
        <rdfs:label xml:lang="en">aggregationLength</rdfs:label>
        <skos:definition xml:lang="en">
            A one dimensional spatial range over which a statistic is calculated, for example
            one metre.
        </skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#aggregation"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#aggregationTimePeriod -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#aggregationTimePeriod">
        <rdfs:label xml:lang="en">aggregationTimePeriod</rdfs:label>
        <skos:definition xml:lang="en">
            A temporal range over which the statistic is calculated. e.g. A day, an hour
        </skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#aggregation"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#aggregationVolume -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#aggregationVolume">
        <rdfs:label xml:lang="en">aggregationVolume</rdfs:label>
        <skos:definition xml:lang="en">
            A three dimensional spatial range over which a statistic is calculated, for example
            one cubic metre.
        </skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#aggregation"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#aopLabel -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#aopLabel">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#prefLabel" />
        <rdfs:label xml:lang="en">label</rdfs:label>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#AbstractObservableProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#label"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#basePhenomenon -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#basePhenomenon">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#categoryValue -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#categoryValue">
        <rdfs:label xml:lang="en">value</rdfs:label>
        <skos:definition xml:lang="en">
            The value of the property that is constrained - e.g. &quot;blue&quot; (if the 
            constrained property is colour&quot;
        </skos:definition>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#CategoryConstraint"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#value"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#constraintBasePhenomenon -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#constraintBasePhenomenon">
        <rdfs:label>Constrained Property</rdfs:label>
        <skos:notation rdf:datatype="http://vocab.nerc.ac.uk/datatypes/datatypes.xml#OPMClassName">
            constrainedProperty
        </skos:notation>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#Constraint"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#basePhenomenon"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#constraintLabel -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#constraintLabel">
        <rdfs:label xml:lang="en">description</rdfs:label>
        <skos:definition xml:lang="en">
            A human readable title for the constraint as a whole
        </skos:definition>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#Constraint"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#label"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#count -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#count">
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#CompositeObservableProperty"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#derivedFrom -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#derivedFrom">
        <rdfs:label xml:lang="en">derivedFrom</rdfs:label>
        <skos:definition xml:lang="en">
            One statistical measure may be derived from another. e.g. Monthly Maximum 
            Temperature may be derived from Daily Mean Temperatures.
        </skos:definition>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#StatisticalMeasure"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#description -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#description">
        <rdfs:label xml:lang="en">description</rdfs:label>
        <skos:definition xml:lang="en">A description of the constraint</skos:definition>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#OtherConstraint"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#label -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#label">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#prefLabel" />
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#opBasePhenomenon -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#opBasePhenomenon">
        <rdfs:label xml:lang="en">basePhenomenon</rdfs:label>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#ObservableProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#basePhenomenon"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#otherAggregation -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#otherAggregation">
        <rdfs:label xml:lang="en">otherAggregation</rdfs:label>
        <skos:definition xml:lang="en">
            Any other type of aggregation
        </skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#aggregation"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#rangeEnd -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#rangeEnd">
        <rdfs:label xml:lang="en">rangeEnd</rdfs:label>
        <skos:definition xml:lang="en">
            The upper limit of the range
        </skos:definition>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#RangeBounds"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#rangeStart -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#rangeStart">
        <rdfs:label xml:lang="en">rangeStart</rdfs:label>
        <skos:definition xml:lang="en">
            The lower limit of the range
        </skos:definition>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#RangeBounds"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#scalarValue -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#scalarValue">
        <rdfs:label xml:lang="en">value</rdfs:label>
        <skos:definition xml:lang="en">
            The numerical value of the property that is constrained
        </skos:definition>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#ScalarConstraint"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#value"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#statisticalMeasure -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#statisticalMeasure">
        <rdfs:label xml:lang="en">statisticalMeasure</rdfs:label>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#related" />
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#statmLabel -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#statmLabel">
        <rdfs:label xml:lang="en">label</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#prefLabel" />
        <skos:definition xml:lang="en">
            A human readable title for the statistical measure
        </skos:definition>
        <rdfs:domain rdf:resource="http://onto.nerc.ac.uk/cpm#StatisticalMeasure"/>
        <rdfs:subPropertyOf rdf:resource="http://onto.nerc.ac.uk/cpm#label"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.nerc.ac.uk/cpm#value -->

    <owl:DatatypeProperty rdf:about="http://onto.nerc.ac.uk/cpm#value"/>
    


    <!-- 
